<?xml version="1.0" encoding="ASCII"?>
<OryxDTSM:DIA
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:OryxDTSM="platform:/resource/OryxModelTransformation/TargerMetaModel(DiceDTSM)/OryxDTSM.ecore"
    xsi:schemaLocation="platform:/resource/OryxModelTransformation/TargerMetaModel(DiceDTSM)/OryxDTSM.ecore OryxDTSM.ecore">
  <DIASer>
    <SerUseKf>
      <HasUpdateTopic/>
      <BrokerURL>broker0.kafka:2192</BrokerURL>
      <KafkaHasInputTopic/>
    </SerUseKf>
    <SpeYarn
        xsi:type="OryxDTSM:ConfSpecification"/>
  </DIASer>
  <DIABa>
    <BatchUseKaf>
      <HasUpdateTopic>
        <UpdatTopichasMaster>
          <zkServers>zk.server:9021</zkServers>
        </UpdatTopichasMaster>
      </HasUpdateTopic>
      <BrokerURL>broker0.example.com:9021</BrokerURL>
      <KafkaHasInputTopic>
        <InputTopichasMaster>
          <zkServers>zk.server:9021</zkServers>
        </InputTopichasMaster>
      </KafkaHasInputTopic>
    </BatchUseKaf>
    <BatchhasSparkStream
        IntervalBtwnComputation="65000"
        master="yarn"
        ExecutorCore="1"
        ExecutorMemory="1g"
        HeapSize="500m"/>
  </DIABa>
  <DIASp>
    <SpeUseKaf>
      <HasUpdateTopic>
        <UpdatTopichasMaster>
          <zkServers>xk-example.com:9021</zkServers>
        </UpdatTopichasMaster>
      </HasUpdateTopic>
      <BrokerURL>broker1.example:9021</BrokerURL>
      <KafkaHasInputTopic>
        <InputTopichasMaster>
          <zkServers>ex.example.com:2021</zkServers>
        </InputTopichasMaster>
      </KafkaHasInputTopic>
    </SpeUseKaf>
  </DIASp>
</OryxDTSM:DIA>
