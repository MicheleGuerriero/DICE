<?xml version="1.0" encoding="ISO-8859-1"?>
<it.polimi.modaclouds.resourcemodel.cloud:Posi_DDSM
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:it.polimi.modaclouds.resourcemodel.cloud="http://cloud/1.0"
    type="cloudify_dsl_1_0">
  <hasLinuxServer1
      name="linuxServer1"
      net_uuid="netuuidvalue">
    <contains_config
        name="config"
        type="config type">
      <relationships
          type="config"
          target="server1"/>
    </contains_config>
    <contains_parsers
        name="idofparser1"
        type="idofparser1"
        InputRate="1256"
        MaxPriority="maxpriorutyThread"/>
    <contains_parsers
        name="idodparser2"
        type="idodparser2"
        InputRate="500"
        MaxPriority="maxPriority"/>
    <contains_server1_chef
        name="server1_chef"
        type="server1_chef">
      <chef_cook_books>recipe[apt::default]</chef_cook_books>
      <chef_cook_books>recipe[java::default]</chef_cook_books>
      <chef_cook_books>recipe[server1::default]</chef_cook_books>
      <chef_cook_books>recipe[RabitMQ::default]</chef_cook_books>
      <chef_cook_books>recipe[hadoop::spark_master]</chef_cook_books>
      <chef_cook_books>recipe[hadoop::hadoop_hdfs_namenode]</chef_cook_books>
      <chef_cook_books>recipe[hadoop::hadoop_yarn_resourcemanager]</chef_cook_books>
    </contains_server1_chef>
    <containsEventIntegrator
        name="eventintegratorName"/>
    <contains_RabitMQ
        rabbitmqNodeIpAddress="10.20.30.40"
        rabbitmqNodePport="2023"
        rabbitmqDistPort=""
        rabbitmqNodename="rabitMQ"
        rabbitmqUseLongname="rabitMQlongname"
        hostName=""/>
  </hasLinuxServer1>
  <hasLinuxServer2
      name="linuxServer2">
    <contains_server2_chef
        name="server2_chef"
        type="server2_chef">
      <chef_cook_books>recipe[apt::default]</chef_cook_books>
      <chef_cook_books>recipe[java::default]</chef_cook_books>
      <chef_cook_books>recipe[server2::default]</chef_cook_books>
    </contains_server2_chef>
    <hasCEP
        name="CEP"
        type="cloudify.nodes.comptation"/>
  </hasLinuxServer2>
  <imports>http://www.getcloudify.org/spec/cloudify/3.3m4/types.yaml</imports>
  <imports>https://raw.githubusercontent.com/buhanec/cloudify-chef-plugin/devel/plugin.yaml</imports>
  <imports>https://raw.githubusercontent.com/buhanec/cloudify-flexiant-plugin/3.3m2/plugin.yaml</imports>
  <hasoutputs
      name="server1IPaddress"/>
  <hasoutputs
      name="Server2IPaddress"/>
  <hasoutputs
      name="PMS_IP_Address"/>
  <hasinputs
      name="chef_version"
      value="{}"
      description=""/>
  <hasinputs
      name="chef_server"
      value="{}"/>
  <hasinputs
      name="chef_validation_user"
      value="{}"/>
  <hasPMS_ApplicationSrv>
    <containsPMS
        name="pmsname"/>
    <contains_applicationSrv_chef
        name="Application Server Chef">
      <chef_cook_books>recipe[PosiAppServer::default]</chef_cook_books>
    </contains_applicationSrv_chef>
  </hasPMS_ApplicationSrv>
  <hasDatabaseServer>
    <containsOracleDB
        name="relationalDatabase"/>
    <contains_DB_chef
        name="oracleServerChef">
      <chef_cook_books>recipe[oracle::default]</chef_cook_books>
    </contains_DB_chef>
  </hasDatabaseServer>
</it.polimi.modaclouds.resourcemodel.cloud:Posi_DDSM>
